<table>
  <tr>
    <th>socket id</th>
    <th>name</th>
    <th>user</th>
    <th>workers</th>
  </tr>
  <%= for {_k, client} <- @workers.clients do %>
    <tr>
      <td><%= client.socket_id %></td>
      <td><%= client.name %></td>
      <td><%= client.user %></td>
      <td><%= client.workers %></td>
    </tr>
  <% end %>
</table>

<script defer type="text/javascript" src='<%= Routes.static_path(@socket, "/js/app.js") %>'></script>
