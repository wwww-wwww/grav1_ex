<div><span>Capacity: </span><span><%= @max_workers - length(@workers) %>/<%= @max_workers %></span></div>
<table>
  <tr>
    <th>segment</th>
    <th>pass</th>
    <th>progress</th>
  </tr>
  <%= for worker <- @workers do %>
    <tr>
      <td>
        <%= worker.segment %>
      </td>
      <td>pass <%= worker.pass %></td>
      <td><%= worker.progress_num %> / <%= worker.progress_den %></td>
    </tr>
  <% end %>
</table>
